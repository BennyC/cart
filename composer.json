{
    "name": "konekt/cart",
    "description": "Cart Module",
    "type": "library",
    "license": "MIT",
    "keywords": ["cart", "ecommerce", "konekt", "artkonekt", "laravel", "eloquent"],
    "support": {
        "source": "https://github.com/artkonekt/cart"
    },
    "authors": [
        {
            "name": "Attila Fulop",
            "homepage": "https://github.com/fulopattila122"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "konekt/concord": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.2",
        "orchestra/testbench": "^3.4.2"
    },
    "autoload": {
        "psr-4": { "Konekt\\Cart\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "Konekt\\Cart\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
